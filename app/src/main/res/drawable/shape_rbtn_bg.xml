<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 第一层  上部和左部偏移一定距离-->
    <item
        android:left="0dp"
        android:top="0dp">
        <shape>
            <solid android:color="@color/blue14" />
            <!-- 描边 -->
            <stroke
                android:width="1dp"
                android:color="@color/blue14">
            </stroke>
            <corners android:radius="8dp"/>
        </shape>
    </item>
